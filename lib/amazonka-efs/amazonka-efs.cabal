        , Network.AWS.EFS.Types.Key
        , Network.AWS.EFS.Types.Value
        , Network.AWS.EFS.Types.OwnerId

    build-depends:
          amazonka-core == 2.0.0.0.*

test-suite amazonka-efs-test
    import:            base
    type:              exitcode-stdio-1.0
    hs-source-dirs:    test
    main-is:           Main.hs

    ghc-options:       -threaded

    -- This section is encoded by the template and any modules added by
    -- hand outside these namespaces will not correctly be added to the
    -- distribution package.
    other-modules:
          Test.AWS.EFS
        , Test.AWS.Gen.EFS
        , Test.AWS.EFS.Internal

    build-depends:
          amazonka-core == 2.0.0.0.*
        , amazonka-test == 2.0.0.0.*
        , amazonka-efs
        , bytestring
        , case-insensitive
        , tasty
        , tasty-hunit
        , text
        , time
        , unordered-containers
